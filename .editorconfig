# editorconfig.org
root = true

[*.xml]
indent_style = space
indent_size = 1
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false

[Makefile]
indent_style = tab
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false
